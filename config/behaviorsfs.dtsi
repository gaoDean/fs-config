    if_shift_dw: if_shift_bksp_dw {
        compatible = "zmk,behavior-mod-morph";
        label = "IF_SHIFT_BKSP_DW";
        #binding-cells = <0>;
        bindings = <&macro_dw>, <&kp BKSP>;
        mods = <(MOD_LSFT|MOD_RSFT)>;
    };

