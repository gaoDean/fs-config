    // A hold-tap that activates without delay (like `&mo`)
    mo_tap: mo_with_tap {
      compatible = "zmk,behavior-hold-tap";
      label = "MO_WITH_TAP";
      #binding-cells = <2>;
      tapping_term_ms = <0>;
      retro-tap;
      flavor = "hold-preferred";
      bindings = <&mo>, <&kp>;
    };

    kr_to: key_repeat_to_layer {
        compatible = "zmk,behavior-hold-tap";
        label = "KEY_REPEAT_TO_LAYER";
        #binding-cells = <2>;
        tapping_term_ms = <150>;
        bindings = <&mo>, <&key_repeat>;
    };

